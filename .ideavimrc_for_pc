let mapleader = "\"
:map <leader>f :action CloseAllEditorsButActive<CR>
:map <leader>c :action CloseContent<CR>
:map <leader><Esc> :action HideAllWindows<CR>
:map <leader>e :action GotoImplementation<CR>
:map <leader>s :action GotoSuperMethod<CR>
